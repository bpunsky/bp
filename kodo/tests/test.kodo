
foreign: "stdio.h"
foreign: "stdint.h"

type int:    #int64;
type i64:    #int64;
type i32:    #int32;
type i16:    #int16;
type i8:     #int8;

type uint:   #uint64;
type u64:    #uint64;
type u32:    #uint32;
type u16:    #uint16;
type u8:     #uint8;

type bool:   #bool64;
type b64:    #bool64;
type b32:    #bool32;
type b16:    #bool16;
type b8:     #bool8;

type f64:    #float64;
type f32:    #float32;

type string: #string;
type ztring: #ztring;

var foo: int = 123;
var bar: int = 321;

if true {
    foo = foo + bar * 123 / 5;
}

for var i := 0; i < 10; i += 1 {
    bar += 2;
}

const print_int "printf": proc(fmt: ztring, i: int);

print_int("%lld\n", foo);
print_int("%lld\n", bar);
print_int("%lld\n", foo + bar);
